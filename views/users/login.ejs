<!DOCTYPE html>
<html lang="en">
    <head>
        <!-- Meta Data -->
        <meta charset="UTF-8">
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <title>login in </title>    
        <!-- Favicon -->
        <link rel="apple-touch-icon" sizes="180x180" href="/TeamPhoto/favicon_io (4)/apple-touch-icon.png">
        <link rel="icon" type="image/png" sizes="32x32" href="/TeamPhoto/favicon_io (4)/favicon-32x32.png">
        <link rel="icon" type="image/png" sizes="16x16" href="/TeamPhoto/favicon_io (4)/favicon-16x16.png">
        <link rel="manifest" href="/site.webmanifest">
    
        <!-- Main Theme Js -->
        <script async="" src="https://www.clarity.ms/s/0.7.24/clarity.js"></script><script async="" src="https://www.clarity.ms/tag/kgy9ps0jhi"></script>
        <script src="https://live.memzo.ai/adminapp/assets/js/authentication-main.js"></script>
    
        <!-- Bootstrap Css -->
        <link id="style" href="https://live.memzo.ai/adminapp/assets/libs/bootstrap/css/bootstrap.min.css" rel="stylesheet">
    
        <!-- Style Css -->
        <link href="https://live.memzo.ai/adminapp/assets/css/styles.min.css" rel="stylesheet">
    
        <!-- Icons Css -->
        <link href="https://live.memzo.ai/adminapp/assets/css/icons.min.css" rel="stylesheet">
        <link href="https://live.memzo.ai/adminapp/assets/css/custom.css" rel="stylesheet">
        
        <link rel="preconnect" href="https://fonts.googleapis.com">
        <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin="">
        <link href="https://fonts.googleapis.com/css2?family=Inter:wght@100;200;300;400;500;600;700;800;900&amp;family=Poppins:ital,wght@0,200;0,300;0,400;0,500;0,600;1,300;1,400;1,500&amp;display=swap" rel="stylesheet">
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.4/jquery.min.js"></script>
    <style type="text/css">
        @media (min-width: 1400px)
        {
    .container, .container-lg, .container-md, .container-sm, .container-xl, .container-xxl {
        width: 95% !important;
        max-width: 1200px !important;
    }
        }
    </style>
    <style>
        .required {
         border: 1px solid red !important;
        }
      </style>

    </head>


<body style="padding: 5px 10px;
    background: linear-gradient(-45deg, #F5ABCA, #FF7783, #FFBB8A, #F5ABCA);
    background-size: 400% 400%;
    animation: gradient 15s ease infinite;">
    
    <% if(success && success.length){ %>
        <div class="alert alert-dark alert-dismissible fade show col-6 offset-3" role="alert">
            <span class="text-green-500"><%= success %></span>
            <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close" style="rotate: 45deg;"><i class="bi bi-plus"></i></button>
        </div>
    <%}%>

    <% if(error && error.length){ %>
        <div class="alert alert-danger alert-dismissible fade show col-6 offset-3" role="alert">
            <%= error %>
            <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
        </div>
    <%}%>
    




    <section class="signin_section" style="background: none !important;">
        <div class="container">
            <div class="row">
                <div class="col-xxl-6 col-xl-6 col-lg-6 col-md-6 text-center left_content position-relative">
                    <!-- <img src="https://live.memzo.ai/adminapp/assets/images/memzo/memzo_logo.png" class="img_logo"> -->
                    <div class="for_mobile_view">
                        <div class="my-4">
                            <h2 class="fs-3 mb-5"> Welcome to Home Away</h2>
                            <img src="/img/image6.webp" class="signin_left_logo w-70 ">
                            
                        </div>
                        <h1>
                            Book your resort now !!
                        </h1>
                        <p>
                          
                        </p>
                    </div>
                </div>
                <div class="col-xxl-6 col-xl-6 col-lg-6 col-md-6 right_content text-center">
                    <div id="signIn">
                        <h3>Sign in</h3>
                        <div class="row">
                            <div class="col-12">
                                <a href="/auth/google" type="button" class="btn w-100 google_facebook_btn">
                                    <img src="https://live.memzo.ai/adminapp/assets/images/memzo/flat-color-icons_google.svg" class="mr-1 align-middle">
                                   Google
                                </a>
                            </div>
                        </div>
                        <h6><span>OR</span></h6>
                        <div class="signin_tab">
                            
                            <div class="tab-content" id="nav-tabContent">
                                <div class="tab-pane fade active show" id="nav-home" role="tabpanel" aria-labelledby="nav-home-tab">

                                    <form method="POST" id="login_form" action="/login" class="needs-validation" novalidate autocomplete="off">

                                        <div class="row mb-3">
                                            <label for="log_email" class="col-sm-12 col-form-label">Username</label>
                                            <div class="col-sm-12">
                                                <input type="text" class="form-control" id="log_username" name="username" placeholder="Enter Username" required  autocomplete="off">
                                            </div>
                                        </div>
                                        <div class="row mb-3">
                                            <label for="log_password" class="col-sm-12 col-form-label">Password</label>
                                            <div class="col-sm-12">
                                                <input type="password" class="form-control" id="log_password" name="password" placeholder="Enter your strong password" required autocomplete="off" >
                                            </div>
                                        </div>
                                        <div class="row mb-3 align-items-center">
                                            <!-- <div class="col-7 text-start">
                                                <a onclick="//show_hide('#forgotpwd','#signIn')//">Forgot password?</a>
                                            </div> -->
                                            <div class="col-7 text-end">
                                                <button id="btn_login" style="width: auto;" onclick="//gologin()//" class="btn signin_btn">
                                                    Sign in
                                                </button>
                                            </div>
                                        </div>
                                    </form>


                                </div>
                            </div>
                            <hr style="margin: 20px 0;">
                            <p>
                                Not registered?
                                <a onclick="show_hide('#signUp','#signIn')" class="cursor-pointer">
                                    Sign up
                                    <svg width="20" height="20" viewBox="0 0 20 20" fill="none" xmlns="http://www.w3.org/2000/svg">
                                        <path d="M14.0575 4.74219L13.1737 5.62605L16.9236 9.37594H0.625V10.626H16.9234L13.1737 14.3757L14.0575 15.2596L19.3163 10.0009L14.0575 4.74219Z" fill="#008D9A"></path>
                                    </svg>
                                </a>
                            </p>
                        </div>
                    </div>
                    <div id="signUp" style="display: none;">
                        <h3>Sign Up</h3>
                        <div class="row">
                            <div class="col-12">
                                  <a href="/auth/google" type="button" class="btn w-100 google_facebook_btn">
                                    <img src="https://live.memzo.ai/adminapp/assets/images/memzo/flat-color-icons_google.svg" class="mr-1 align-middle">
                                   Google
                                </a>
                            </div>
                            
                        </div>
                        <h6><span>OR</span></h6>
                        <h3 class="m-0">Create new account</h3>
                        <div class="signin_tab">

                            <form id="signupForm" action="/signUp" method="POST" class="needs-validation" autocomplete="off">
                                <div class="row mb-3">
                                    <label for="Username" class="col-sm-12 col-form-label">Username</label>
                                    <div class="col-sm-12">
                                        <input type="text" class="form-control" id="signup_Username" name="username" placeholder="Enter Your Username" required autocomplete="new-username">
                                    </div>
                                    <p class="mt-1 mb-0" style="color: red; font-size: x-small; text-align: start;">Enter UserName carefully. It will not change in future.*</p>
                                </div>
                                <div class="row mb-3">
                                    <label for="inputEmail3" class="col-sm-12 col-form-label">Email</label>
                                    <div class="col-sm-12">
                                        <input type="email" class="form-control" id="signup_email" name="email" placeholder="Enter Your E-Mail" required>
                                    </div>
                                </div>
                                <div class="row mb-3">
                                    <label for="inputPassword3" class="col-sm-12 col-form-label">Password</label>
                                    <div class="col-sm-12">
                                        <input type="password" class="form-control" id="signup_Password" name="password" placeholder="Enter Password" autocomplete="new-password" required minlength="8">
                                    </div>
                                </div>
                                <div class="row mb-3 align-items-center">
                                    <div class="col-sm-12 text-end">
                                        <button type="submit" id="signup_btn_submit" class="btn signin_btn w-100">
                                            Create Account
                                        </button>
                                    </div>
                                </div>
                            </form>
                            
                        
                            


                            <hr style="margin: 20px 0;">
                            <p>
                                Already registered?
                                <a onclick="show_hide('#signIn','#signUp')" class="cursor-pointer">
                                    Sign In
                                    <svg width="20" height="20" viewBox="0 0 20 20" fill="none" xmlns="http://www.w3.org/2000/svg">
                                        <path d="M14.0575 4.74219L13.1737 5.62605L16.9236 9.37594H0.625V10.626H16.9234L13.1737 14.3757L14.0575 15.2596L19.3163 10.0009L14.0575 4.74219Z" fill="#008D9A"></path>
                                    </svg>
                                </a>
                            </p>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>

    <script>
        document.getElementById('signupForm').addEventListener('submit', function(event) {
            if (!this.checkValidity()) {
                event.preventDefault();
                event.stopPropagation();
            }
            this.classList.add('was-validated');
        }, false);
        </script>



    <script>
        
        window.onload = function() {
      document.getElementById("signupForm").reset();
      document.getElementById("login_form").reset();
    }
    document.getElementById("signup_Username").value = "";
    document.getElementById("signup_email").value = "";
    document.getElementById("signup_Password").value = ""; 

    document.getElementById("log_username").value = ""; 
    document.getElementById("log_password").value = ""; 
    </script>
    
    <!-- Custom-Switcher JS -->
    <script src="https://live.memzo.ai/adminapp/assets/js/custom-switcher.min.js"></script>

    <!-- Bootstrap JS -->
    <script src="https://live.memzo.ai/adminapp/assets/libs/bootstrap/js/bootstrap.bundle.min.js"></script>

    <!-- Show Password JS -->
    <script src="https://live.memzo.ai/adminapp/assets/js/show-password.js"></script>

    <!-- Form Validation -->
    <script src="/JS/FormValidation.js"></script>

    <script>

        $('.numberEdit_div').hide();
       $('.forgotpwd').hide();
       $('.forgotpwdReset').hide();
        
        function showSignUp_div() {
           
            $('#signUp').show();
            $('#signIn').hide();
            $('#forgotpwd').hide();
            $('#forgotpwdReset').hide();
            $('#signupVerify').hide();
           
        }
        function showSignIn_div() {
            $('#signUp').hide();
            $('#signIn').show();
            $('#forgotpwd').hide();
            $('#forgotpwdReset').hide();
            $('#signupVerify').hide();
           
        }

        function show_hide(elm1, elm2)
        {
            $(elm1).show();
            $(elm2).hide();
        }


    </script>
    <!-- Jquery -->
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.4/jquery.min.js"></script>
    <script>
        $(document).ready(function(){
            setTimeout(function(){
                $('.alert').alert('close');
            }, 2000);
        });
    </script>

</body>
    

</body>
</html>


   
     
      
  

